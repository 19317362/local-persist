{
  "description": "Create named local volumes that persist in the location(s) you want",
  "documentation": "https://github.com/CWSpear/local-persist#usage-creating-volumes",
  "args": {
    "description": "",
    "name": "",
    "settable": null,
    "value": null
  },
  "entrypoint": [
    "/usr/bin/docker-volume-local-persist"
  ],
  "env": [
    {
      "description": "",
      "name": "DEBUG",
      "settable": [
        "value"
      ],
      "value": "0"
    }
  ],
  "interface": {
    "socket": "local-persist.sock",
    "types": [
      "docker.volumedriver/1.0"
    ]
  },
  "linux": {
    "capabilities": null,
    "allowAllDevices": false,
    "devices": null
  },
  "mounts": [
    {
      "destination": "/var/lib/docker/plugin-data/",
      "options": [
        "rbind"
      ],
      "name": "local-persist.json",
      "source": "/var/lib/docker/plugins/",
      "settable": [
        "source"
      ],
      "type": "bind"
    }
  ],
  "network": {
    "type": ""
  },
  "propagatedMount": "/data",
  "user": {},
  "workdir": ""
}
